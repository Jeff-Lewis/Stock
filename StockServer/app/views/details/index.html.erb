<h1>Listing details</h1>

<table>
  <tr>
    <th>Stock</th>
    <th>Exchange</th>
    <th>Yearmin</th>
    <th>Yearmax</th>
    <th>Avgvol</th>
    <th>Marketcap</th>
    <th>Pe</th>
    <th>Eps</th>
    <th>Divyield</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @details.each do |detail| %>
  <tr>
    <td><%= detail.stock.symbol %></td>
    <td><%= detail.stock.exchange.name %></td>
    <td><%= detail.yearMin %></td>
    <td><%= detail.yearMax %></td>
    <td><%= detail.avgVol %></td>
    <td><%= detail.marketCap %></td>
    <td><%= detail.pe %></td>
    <td><%= detail.eps %></td>
    <td><%= detail.divYield %></td>
    <td><%= link_to 'Show', detail %></td>
    <td><%= link_to 'Edit', edit_detail_path(detail) %></td>
    <td><%= link_to 'Destroy', detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

