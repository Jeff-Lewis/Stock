[
    <% @erdates.each_with_index do |erdate,index| %>
    {
        erdate: <%= erdate.to_json().html_safe() %>,
        stock: <%= erdate.stock.to_json().html_safe() %>,
        popularity: <%= erdate.stock.popularity.to_json().html_safe() %>,
        watch: <%= watch?(@user, erdate) %>
    }
